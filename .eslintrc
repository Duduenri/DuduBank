{
    "extends": [
    "eslint:recommend",
    "plugin:import/errors",
    "plugin:react/recommended",
    "plugin:jsx-a11y/recommend",
    "plugin:react-hooks/recommend", 
    "prettier"
],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "es6": true,
        "browser": true,
        "jest": true,
        "node": true
    }
}